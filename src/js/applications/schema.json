{
    "name": {
        "out": ["first_name", "last_name"],
        "input": "text",
        "label": "Name",
        "error": "Name (First & Last)",
        "placeholder": "First Last"
    },

    "phone": {
        "input": "text",
        "label": "phone",
        "error": "Phone",
        "placeholder": "555-556-2020"
    },

    "birthday": {
        "out": ["birth_day", "birth_month", "birth_year"],
        "input": "date",
        "label": "Birthday",
        "error": "Birthday (Above 13)"
    },

    "race": {
        "input": "list-other-select",
        "label": "Race"
    },

    "gender": {
        "input": "other-select",
        "label": "Gender"
    },



    "city": {
        "out": "travel_origin",
        "input": "autocomplete-text",
        "label": "City",
        "placeholder": "East Lansing"
    },
    
    "university": {
        "input": "opt-autocomplete-text",
        "label": "University",
        "placeholder": "Michigan State University"
    },
    
    "major": {
        "input": "list-autocomplete-text",
        "label": "Major",
        "Error": "Major (From Autocomplete)",
        "placeholder": "Your Major"
    },
    
    "gradYear": {
        "out": "graduation_year",
        "input": "number",
        "inputOpts": {
            "min": 2020,
            "max": 2025
        },
        "label": "Graduation Year",
        "validator": false
    },
    "gradSeason": {
        "out": "graduation_seson",
        "input": "select",
        "label": "Graduation Season",
        "validator": false
    },



    "github": {
        "input": "text",
        "label": "Github",
        "labelVis": "inline",
        "placeholder": "profile || paste",
        "optional": true
    },

    "linkedin": {
        "input": "inlineLabel-text",
        "label": "LinkedIn",
        "placeholder": "profile || paste",
        "optional": true
    },

    "devpost": {
        "input": "text",
        "label": "LinkedIn",
        "labelVis": "inline",
        "placeholder": "profile || paste",
        "optional": true
    },

    "website": {
        "out": ["website", "other_link"],
        "input": "text",
        "label": "LinkedIn",
        "placeholder": "your-site.com",
        "optional": true
    },

    "mlhExperience": {
        "out": "hackathons",
        "input": "number",
        "inputOpts": { "min": 0 },
        "label": "hidden",
        "labelVis": "hidden",
        "error": "MLH Experience",
        "validator": false
    },

    "personalStatement": {  
        "out": "statement",
        "input": "paragraph-text",
        "label": "Personal Statement",
        "placeholder": "> 24 Words"
    }
}